{
    "name": "masterclass",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check \"build-only {@}\" --",
        "preview": "vite preview",
        "build-only": "vite build",
        "type-check": "vue-tsc --build --force",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "format": "prettier --write src/",
        "supabase:init": "supabase init",
        "supabase:login": "supabase login",
        "supabase:logout": "supabase logout",
        "supabase:link": "supabase link  --project-ref $SUPABASE_PROJECT_ID",
        "db:migrate:new": "supabase migration new",
        "db:reset": "supabase db reset --linked",
        "db:seed": "node --env-file=.env database/seed.js",
        "subabase:generate-types": "npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > database/types.ts"
    },
    "dependencies": {
        "@supabase/supabase-js": "^2.45.6",
        "@vueuse/core": "^11.1.0",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "lucide-vue-next": "^0.453.0",
        "pinia": "^2.1.7",
        "radix-vue": "^1.9.7",
        "tailwind-merge": "^2.5.4",
        "tailwindcss-animate": "^1.0.7",
        "vue": "^3.4.29",
        "vue-router": "^4.3.3"
    },
    "devDependencies": {
        "@faker-js/faker": "^9.0.3",
        "@rushstack/eslint-patch": "^1.8.0",
        "@tsconfig/node20": "^20.1.4",
        "@types/node": "^20.14.5",
        "@vitejs/plugin-vue": "^5.0.5",
        "@vue/eslint-config-prettier": "^9.0.0",
        "@vue/eslint-config-typescript": "^13.0.0",
        "@vue/tsconfig": "^0.5.1",
        "autoprefixer": "^10.4.20",
        "eslint": "^8.57.0",
        "eslint-plugin-vue": "^9.23.0",
        "npm-run-all2": "^6.2.0",
        "prettier": "^3.2.5",
        "supabase": "^1.204.3",
        "tailwindcss": "^3.4.14",
        "typescript": "~5.4.0",
        "unplugin-vue-router": "^0.10.8",
        "vite": "^5.3.1",
        "vue-tsc": "^2.0.21"
    }
}
